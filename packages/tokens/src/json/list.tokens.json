{
  "$schema": "https://design-tokens.github.io/community-group/format/",
  "$description": "UIID Design System - List Component Tokens",
  "list": {
    "padding": {
      "x": { "$value": "0.5rem", "$type": "dimension" },
      "y": { "$value": "0.5rem", "$type": "dimension" }
    },
    "group": {
      "category": {
        "minHeight": { "$value": "1.5rem", "$type": "dimension" }
      }
    },
    "item": {
      "padding": {
        "x": { "$value": "0.5rem", "$type": "dimension" },
        "y": { "$value": "0.5rem", "$type": "dimension" }
      }
    }
  }
}
