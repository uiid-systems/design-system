{
  "name": "@uiid/registry",
  "version": "0.0.4",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "tsc -b --watch & vite build --watch",
    "watch": "tsc -b --watch & vite build --watch",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@uiid/utils": "workspace:*",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@uiid/buttons": "workspace:*",
    "@uiid/cards": "workspace:*",
    "@uiid/forms": "workspace:*",
    "@uiid/layout": "workspace:*",
    "@uiid/typography": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "prettier": "3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-tsconfig-paths": "^6.0.5"
  }
}
