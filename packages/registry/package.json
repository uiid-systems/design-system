    "@uiid/forms": "workspace:*",
    "@uiid/layout": "workspace:*",
    "@uiid/typography": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.1",
    "globals": "^17.3.0",
    "prettier": "3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
