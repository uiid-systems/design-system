{
  "name": "@uiid/registry",
  "version": "0.0.7",
  "description": "Component metadata, Zod schemas, and AI-friendly documentation",
  "license": "MIT",
  "author": "Adam Fratino",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiid-systems/design-system",
    "directory": "packages/registry"
  },
  "homepage": "https://github.com/uiid-systems/design-system/tree/main/packages/registry",
  "bugs": "https://github.com/uiid-systems/design-system/issues",
  "keywords": [
    "uiid",
    "react",
    "components",
    "design-system",
    "registry",
    "schemas",
    "zod"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
